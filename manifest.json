{
    "name": "Export Tabs",
    "version": "1.3.1",
    "description": "Export the URLs of All Open Tabs to Text",
    "manifest_version": 3,
    "icons": {
        "16": "tabs_io.png",
        "48": "tabs_io_48.png",
        "128": "tabs_io_128.png"
    },
    "action": {
        "default_icon": "tabs_io.png",
        "default_title": "Export Tabs",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"]
}
